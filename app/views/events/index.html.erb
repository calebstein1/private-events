<h3>Upcoming Events:</h3>
<ul>
  <% @events.future.each do |event| %>
    <li><%= link_to event.name, event, data: { turbo_frame: "_event-display" } %></li>
  <% end %>
</ul>
<h3>Past Events:</h3>
<ul>
  <% @events.past.each do |event| %>
    <li><%= link_to event.name, event, data: { turbo_frame: "_event-display" } %></li>
  <% end %>
</ul>
