<ul>
  <% if user_signed_in? %>
    <li><%= link_to "Create Event", new_event_path, data: { turbo_frame: "_event-display" } %></li>
    <li><%= link_to "View Profile", "/users/#{current_user.id}", data: { turbo_frame: "_event-display" } %></li>
    <li><%= link_to "Sign Out", destroy_user_session_path, data: { turbo_method: 'delete' } %></li>
  <% else %>
    <li><%= link_to "Sign In/Up", new_user_session_path, data: { turbo_frame: "_event-display" } %></li>
  <% end %>
</ul>